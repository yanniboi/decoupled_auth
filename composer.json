{
  "name": "drupal/decoupled_auth",
  "description": "Allow decoupling of Drupal Authentication from Drupal Users.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/profile": "~1.0",
    "drupal/address": "~1.0"
  },
  "require-dev": {
    "drupal/coder": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\decoupled_auth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\decoupled_auth\\": "tests/src"
    }
  }
}

