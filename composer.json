{
  "name": "drupal/decoupled_auth",
  "description": "Allow decoupling of Drupal Authentication from Drupal Users.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    }
  ],
  "require": {
    "drupal/profile": "8.1.*",
    "drupal/address": "8.1.*"
  },
  "require-dev": {
    "drupal/coder": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\decoupled_auth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\decoupled_auth\\": "tests/src"
    }
  }
}
